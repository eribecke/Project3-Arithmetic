<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tool="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/menu">

    <fragment
        android:id="@+id/menu"
        android:name="edu.iu.eribecke.project3.menu"
        android:label="fragment_menu"
        tool:layout="@layout/fragment_menu">
        <action
            android:id="@+id/action_menuFragment_to_questionsFragment"
            app:destination="@id/questions"/>
        <argument
            android:name="result"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"
            />

    </fragment>
    <fragment
        android:id="@+id/questions"
        android:name="edu.iu.eribecke.project3.questions"
        android:label="fragment_questions"
        tool:layout="@layout/fragment_questions">
        <action
            android:id="@+id/action_questionFragment_to_menuFragment"
            app:destination="@id/menu"/>
        <argument
            app:argType="integer"
            android:name="questionNum" />
        <argument
            android:name="operand"
            app:argType="integer" />
        <argument
            android:name="operator"
            app:argType="string" />
    </fragment>



</navigation>